#pow-nav {
    position: absolute;
    width: 200px;
    height: 70px;
    left: 50%;
    line-height: 0;
    margin-left: -100px;
    padding-top: 20px;
    bottom: 15px;
    opacity: 0;
    -moz-user-select: none;
    cursor: default;
}
#pow-nav .nav.first {
    border-top-left-radius: 10px;
}
#pow-nav .nav, #pow-nav .status {
    margin: 1px;
    background: none repeat scroll 0% 0% rgba(0, 0, 0, 0.85);
    box-shadow: 0px 3px 3px rgba(0, 0, 0, 0.5);
}
#pow-nav .nav {
    display: inline-block;
    width: 48px;
    height: 50px;
}
#pow-nav .nav.last {
    border-top-right-radius: 10px;
}
#pow-nav .status {
    height: 20px;
    line-height: 20px;
    text-align: center;
    color: #AAA;
    font-family: sans-serif;
    border-radius: 0px 0px 10px 10px;
}

